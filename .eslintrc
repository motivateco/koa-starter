{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "extends": [
    "eslint:recommended",
    "standard"
  ],
  "env": {
    "node" : true
  },
  "rules": {
    "indent": ["error", 2, { "MemberExpression": 1 }],
    "max-len": [2, 120, 2],
    "no-var": 2,
    "no-console": 2,
    "prefer-const": 2,
    "object-curly-spacing": [2, "always"],
    "newline-per-chained-call": 2
  }
}
